<script language="JavaScript" type="text/JavaScript">
<!--
//=========================
jQuery (document).ready (function () {
  jQuery ('.SkinImg').click (function() {
    ToggleSkin (jQuery (this));
  })
});

//--------------------------------------------------
function ToggleSkin (img)
{
  if (img.width () < 300)
  {
    ShowSkin (img);
  }
  else
  {
    HideSkin (img);
  }
}

//--------------------------------------------------
function ShowSkin (img)
{
  img.animate ({
    width: '414px',
    height: '294px'
  },
  {
    step: function (now, fx) {
      var frac = now / 300;
      var top = -20 * ( 1 - frac);
      var w = 331 + 83 * frac;
      var h = 235 + 59 * frac;
      fx.elem.style.backgroundPosition = "0px " + top + "px";
      fx.elem.style.backgroundSize = w + "px " + h + "px";
    },
    easing: "swing"
  })
}

//--------------------------------------------------
function HideSkin (img)
{
  img.animate ({
    width: '200px',
    height: '56px'
  },
  {
    step: function (now, fx) {
      var frac = now / 300;
      var top = -20 * ( 1 - frac);
      var w = 331 + 83 * frac;
      var h = 235 + 59 * frac;
      fx.elem.style.backgroundPosition = "0px " + top + "px";
      fx.elem.style.backgroundSize = w + "px " + h + "px";
    },
    easing: "swing"
  })
}
//-->
</script>

